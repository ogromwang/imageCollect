{
  "$schema": "..\\node_modules/@tauri-apps/cli\\schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "yarn run serve --port 3000",
    "devPath": "http://localhost:3000",
    "distDir": "../public"
  },
  "package": {
    "productName": "ImageView",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "protocol": {
        "asset": true,
        "all": true,
        "assetScope": ["*"]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "ImageView",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": "default-src asset: blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fileDropEnabled": false,
        "fullscreen": false,
        "height": 600,
        "resizable": false,
        "title": "ImageView",
        "width": 800
      },
      {
        "fileDropEnabled": false,
        "fullscreen": false,
        "height": 60,
        "url": "/drag",
        "decorations": false,
        "alwaysOnTop": true,
        "resizable": false,
        "title": "Drag",
        "label": "Drag",
        "width": 120,
        "visible": false,
        "theme": "Light",
        "transparent": true
      }
    ],
    "systemTray": {
      "iconPath": "icons/128x128.png",
      "iconAsTemplate": true
    }
  }
}
